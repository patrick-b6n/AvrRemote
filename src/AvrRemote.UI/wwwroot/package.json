{
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "10.4.0",
    "postcss": "8.4.4",
    "postcss-import": "14.0.2",
    "postcss-url": "^10.1.3",
    "tailwindcss": "3.0.0"
  },
  "scripts": {
    "build": "yarn build-css",
    "build-css": "yarn exec tailwindcss --postcss -i ./src/css/tw.css -o ./dist/css/site.min.css",
    "watch-css": "yarn exec tailwindcss --postcss -i ./src/css/tw.css -o ./dist/css/site.min.css --watch",
    "publish": "yarn publish-css",
    "publish-css": "yarn exec tailwindcss --postcss -i ./src/css/tw.css -o ./dist/css/site.min.css --minify "
  },
  "packageManager": "yarn@3.1.0"
}
